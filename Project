CREATE TABLE CIS_ATTRIBUTE_DATA (
    ATTRIBUTE_NAME   VARCHAR2(100),
    ATTRIBUTE_VALUE  VARCHAR2(500),
    SHORT_DESC       VARCHAR2(255),
    LONG_DESC        VARCHAR2(1000),
    CREATE_ID        VARCHAR2(50),
    CREATE_DT        DATE,
    LST_UPD_ID       VARCHAR2(50),
    LST_UPD_DT       DATE,
    VER_NUM          NUMBER,
    ACT_STRT_DT      DATE,
    ACT_END_DT       DATE,
    KYC_SHARD_NUM    NUMBER,
    UNIQUE_ID        VARCHAR2(100),
    LST_UPD_TIME     VARCHAR2(20)
);
