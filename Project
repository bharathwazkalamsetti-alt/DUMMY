ALTER TABLE AMC_ENTITY_GFCID 
  ADD CONSTRAINT AMC_ENTITY_GFCID_PK1 PRIMARY KEY (UNIQUE_ID);

ALTER TABLE AMC_ENTITY_GFCID 
  ADD CONSTRAINT AMC_ENTITY_GFCID_PK_UNIQUE UNIQUE (GFCID);

CREATE INDEX IDX_AMC_ENTITY_GFCID_1 ON AMC_ENTITY_GFCID (CUST_NAME);
CREATE INDEX IDX_AMC_ENTITY_GFCID_2 ON AMC_ENTITY_GFCID (DOMICILECOUNTRYCODE);
CREATE INDEX IDX_AMC_ENTITY_GFCID_3 ON AMC_ENTITY_GFCID (STATUS);
CREATE INDEX IDX_AMC_ENTITY_GFCID_4 ON AMC_ENTITY_GFCID (UPPER(GFCID));
CREATE INDEX IDX_AMC_ENTITY_GFCID_5 ON AMC_ENTITY_GFCID (UPPER(DOMICILECOUNTRYCODE));
CREATE INDEX IDX_AMC_ENTITY_GFCID_6 ON AMC_ENTITY_GFCID (LST_UPD_DT);
CREATE INDEX IDX_AMC_ENTITY_GFCID_7 ON AMC_ENTITY_GFCID (GFCID, CUST_NAME, DOMICILECOUNTRYCODE);
CREATE INDEX IDX_AMC_ENTITY_GFCID_8 ON AMC_ENTITY_GFCID (GFCID, GFPID, STATUS, ACCOUNTTYPE);

