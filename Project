CREATE TABLE COUNTRY_APPX (
    CNTRY_APPX_ID                    VARCHAR2(100),
    COUNTRY_APPX_STATE               VARCHAR2(50),
    KYCID                            VARCHAR2(100),
    CORE_COMPONENT_STATE             VARCHAR2(50),
    CNTRY_CODE                       VARCHAR2(10),
    NEXT_PERIODIC_REVIEW_DT          DATE,
    STATUS                           VARCHAR2(50),
    COUNTRY_APPX_VERSION_NUM         NUMBER,
    COMPLETION_STATUS                VARCHAR2(50),
    RISK_SCORE                       NUMBER(5,2),
    RISK_RATING                      VARCHAR2(20),
    CREATE_ID                        VARCHAR2(50),
    CREATE_DT                        DATE,
    LST_UPD_ID                       VARCHAR2(50),
    LST_UPD_DT                       DATE,
    VER_NUM                          NUMBER,
    ACT_STRT_DT                      DATE,
    ACT_END_DT                       DATE,
    KYC_SHARD_NUM                    NUMBER,
    ANNIVERSARY_DATE                 DATE,
    APPROVED_DATE                    DATE,
    LST_RENEW_TRIG_DT                DATE,
    LST_RENEW_TRIG_TYPE              VARCHAR2(50),
    RISK_CHANGE_IND                  VARCHAR2(5),
    OBLIGOR_NUM                      VARCHAR2(50),
    LST_UPD_TIME                     VARCHAR2(20),
    BRANCH                           VARCHAR2(50),
    PRIM_OWNER_SOEID                 VARCHAR2(50),
    PRIM_OWNER_BUSINESS_UNIT         VARCHAR2(100),
    FORMS_VERSION_NUM                VARCHAR2(50),
    PRIM_OWNER_BUSINESS_SUB_UNIT     VARCHAR2(100),
    IS_CNTRY_RISK_OVERRIDE_APPLIED   VARCHAR2(5),
    LAST_PRCR_WI_APPR_DT             DATE,
    LEGACY_COUNTRY_APPX_ID           VARCHAR2(100),
    LAST_COMPLETED_PRCR_DATE         DATE,
    CR48RULE                         VARCHAR2(50),
    ALL_ACCTS_CLOSED_FLAG            VARCHAR2(5),
    ACCTS_DEACTIVATED_DATE           DATE,
    ACCTS_REACTIVATED_DATE           DATE,
    CORE_MEMBER_LST_INH_DT           DATE,
    IS_EXCEPTION_REQUEST             VARCHAR2(5),
    PREV_ETL_STATUS                  VARCHAR2(50),
    ACCOUNTS_RELATIONSHIP_TYPE       VARCHAR2(50),
    FINCEN_EXMP_BY_SRC               VARCHAR2(50),
    FINCEN_EXMP_BY_SRC_RESN          VARCHAR2(255),
    FINCEN_EXMP_MAN_UPDT             VARCHAR2(50),
    FINCEN_EXMP_MAN_UPDT_RESN        VARCHAR2(255),
    FINCEN_EXMP_MAN_UPDT_DOC_URL     VARCHAR2(500),
    IS_AGGREGATION_REQ               VARCHAR2(5),
    HEAD_OFFICE_GFCID                VARCHAR2(50),
    HEAD_OFFICE_CLIENT_NAME          VARCHAR2(200),
    HEAD_OFFICE_COCS                 VARCHAR2(50),
    IS_AGGREGATION_PERFORMED         VARCHAR2(5),
    BEARER_SHARES_DL NQ_INDICATOR     VARCHAR2(5),
    DEAL_ID                          VARCHAR2(100),
    DEAL_ID_CREATION_TS              DATE,
    LAST_APPROVED_STATUS             VARCHAR2(50),
    DEACTIVATION_DATE                DATE,
    REACTIVATION_DATE                DATE,
    ALL_ACCTS_CLOSED_FLAG_ICG        VARCHAR2(5),
    PORTAL_MESSAGE_STATUS            VARCHAR2(50),
    NRA_GFCID                        VARCHAR2(50),
    SI0722_AGG_STATUS                VARCHAR2(50),
    SI0722_WORK_ITEM_TYPE            VARCHAR2(50),
    SI0722_WORK_ITEM_INITIATION_DT   DATE,
    SI0722_LAST_AGG_DT               DATE,
    PARENT_DEAL_ID                   VARCHAR2(100),
    RISK_OVERRIDE_DT                 DATE,
    RISK_OVERRIDE_REASON             VARCHAR2(255),
    RISK_OVERRIDE_FROM               DATE,
    RISK_OVERRIDE_TO                 DATE,
    RISK_OVERRIDE_COMMENTS           VARCHAR2(1000),
    CALCULATED_RISK_RATING           VARCHAR2(20),
    RISK_OVERRIDE_OTH_COMMENTS       VARCHAR2(500),
    UPLIFTED_FORMS_VERSION_NUM       VARCHAR2(50),
    APPX_RISK_RATING_BD              VARCHAR2(50),
    BASE_RISK_RATING                 VARCHAR2(20),
    GOOGLE_KYC_IND                   VARCHAR2(5),
    IS_GOOGLE_CIN                    VARCHAR2(5),
    IS_GOOGLE_SIMPLIFIED             VARCHAR2(5),
    HRAC_FLAG                        VARCHAR2(5),
    SIMPLIFIED_CUST_FLAG             VARCHAR2(5),
    EXPIRY_TYPE                      VARCHAR2(50),
    SI549_RULE                       VARCHAR2(50),
    SCHEDULED_UNSCHEDULED_PRCR       VARCHAR2(50),
    EXTENSION_EXPIRY_DATE            DATE,
    IS_SANCTIONS_SUSPEND             VARCHAR2(5),
    IS_NDA_FLAG                      VARCHAR2(5),
    LAST_WORK_ITEM_TYPE              VARCHAR2(50),
    LAST_WORK_ITEM_APPROVED_DATE     DATE,
    DEACTIVATED_IN_PRCR              VARCHAR2(5),
    NAIC_CODES                       VARCHAR2(50),
    INCOMPLETE_SOFT_ABANDON_FLAG     VARCHAR2(5),
    DIVESTITURE_FLAG                 VARCHAR2(5),
    LEGAL_VEHICLE                    VARCHAR2(100),
    IS_COMPLETN_STATUS_RESET_REQD    VARCHAR2(5),
    NATURE_OF_BUSINESS               VARCHAR2(255),
    BUSINESS_UNIT                    VARCHAR2(100),
    PRODUCT_SERVICE_CITI             VARCHAR2(100),
    CLIENT_POSITION_CONFLICT         VARCHAR2(5),
    EXCEPTION_END_DATE               DATE,
    IS_IPB_CLIENT                    VARCHAR2(5)
);

-- Test query
SELECT * FROM COUNTRY_APPX;
