CREATE TABLE KYC_BATCH_LOG (
    BATCH_ID           VARCHAR2(100),
    RGN_ID             VARCHAR2(50),
    CNTRY_CD           VARCHAR2(5),
    BATCH_CD           VARCHAR2(50),
    BATCH_DESCR        VARCHAR2(255),
    BATCH_PROCESS_TYPE VARCHAR2(50),
    PROCESS_DATE       DATE,
    TOTAL_PROCESS_CNT  NUMBER,
    SUCCESS_CNT        NUMBER,
    FAILURE_CNT        NUMBER,
    CREATE_ID          VARCHAR2(50),
    CREATE_DT          DATE,
    LST_UPD_ID         VARCHAR2(50),
    LST_UPD_DT         DATE,
    ACT_STRT_DT        DATE,
    ACT_END_DT         DATE,
    VER_NUM            NUMBER,
    KYC_SHARD_NUM      NUMBER,
    LST_UPD_TIME       VARCHAR2(20),
    BATCH_STATUS       VARCHAR2(50)
);
