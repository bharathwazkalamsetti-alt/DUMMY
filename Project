CREATE TABLE KYC_MAIN (
    KYCID                          VARCHAR2(100),
    CORE_COMPONENT_STATE           VARCHAR2(50),
    LEGAL_NAME                     VARCHAR2(200),
    LAST_NAME                      VARCHAR2(100),
    FIRST_NAME                     VARCHAR2(100),
    MIDDLE_NAME                    VARCHAR2(100),
    CLIENT_DOB                     DATE,
    CLIENT_TYPE                    VARCHAR2(50),
    CLIENT_CATEGORY                VARCHAR2(50),
    CLIENT_SEGMENT                 VARCHAR2(50),
    DOMICILE_CNTRY_CODE            VARCHAR2(10),
    CITI_SERVICE_CNTRY_CODE        VARCHAR2(10),
    NEXT_PERIODIC_REVIEW_DT        DATE,
    RISK_SCORE                     NUMBER(5,2),
    RISK_RATING                    VARCHAR2(20),
    COMPLETION_STATUS              VARCHAR2(50),
    EMPLOYEE_FLAG                  VARCHAR2(5),
    EXCLUSION_FLAG                 VARCHAR2(5),
    LEGACY_RISK_RATING             VARCHAR2(20),
    LEGACY_RISK_SCORE              NUMBER(5,2),
    LEGACY_RISK_LST_UPD_DT         DATE,
    STATUS                         VARCHAR2(50),
    KYC_VERSION_NUM                NUMBER,
    RISK_MODEL_TIER                VARCHAR2(50),
    RISK_MODEL_FEED_DT             DATE,
    CORE_TYPE                      VARCHAR2(50),
    REVENUE                        NUMBER,
    HAS_RM                         VARCHAR2(5),
    TRIGGERING_CLIENT_TYPE         VARCHAR2(50),
    NET_WORTH                      NUMBER,
    CREATE_ID                      VARCHAR2(50),
    CREATE_DT                      DATE,
    LST_UPD_ID                     VARCHAR2(50),
    LST_UPD_DT                     DATE,
    VER_NUM                        NUMBER,
    ACT_STRT_DT                    DATE,
    ACT_END_DT                     DATE,
    KYC_SHARD_NUM                  NUMBER,
    MANAGED_CNTRY_CODE             VARCHAR2(10),
    CUSTOMER_SUB_TYPE              VARCHAR2(50),
    MCLE_ID                        VARCHAR2(50),
    ANNIVERSARY_DATE               DATE,
    APPROVED_DATE                  DATE,
    LST_RENEW_TRIG_DT              DATE,
    LST_RENEW_TRIG_TYPE            VARCHAR2(50),
    FOCUS_FLAG                     VARCHAR2(5),
    SDD1                           VARCHAR2(50),
    SDD2                           VARCHAR2(50),
    SDD3                           VARCHAR2(50),
    RISK_CHANGE_IND                VARCHAR2(5),
    HAS_CONSENT                    VARCHAR2(5),
    LEGAL_VEHICLE_ID               VARCHAR2(50),
    SDD2_CNTRY_RISK_RATING         VARCHAR2(20),
    LEGACY_PERIODIC_REVIEW_DUE_DT  DATE,
    AUM_INDV_TRIGGER               VARCHAR2(50),
    AUM_INDV_SOURCE                VARCHAR2(50),
    PRIMARY_CLIENT_ID              VARCHAR2(50),
    PRIMARY_ID_TYPE                VARCHAR2(50),
    GFPID                          VARCHAR2(50),
    PARENT_CUST_NAME               VARCHAR2(200),
    PARENT_DOMICILECOUNTRYCODE     VARCHAR2(10),
    IS_INVESTMENT_CLIENT           VARCHAR2(5),
    IS_INVL_CITI_ACCT_RELN         VARCHAR2(5),
    MULTIPLE_CIN_IND               VARCHAR2(5),
    LST_UPD_TIME                   VARCHAR2(20),
    BRANCH                         VARCHAR2(50),
    PRIM_OWNER_SOEID               VARCHAR2(50),
    PRIM_OWNER_BUSINESS_UNIT       VARCHAR2(100),
    FORMS_VERSION_NUM              VARCHAR2(50),
    PRIM_OWNER_BUSINESS_SUB_UNIT   VARCHAR2(100),
    IS_NEW_CLIENT                  VARCHAR2(5),
    LAST_CORE_COMMENTS             VARCHAR2(500),
    LAST_WORK_ITEM_STATUS          VARCHAR2(50),
    LAST_WORK_ITEM_COMPLETION_DATE DATE,
    LAST_WORK_ITEM_CREATION_DATE   DATE,
    LAST_WORK_ITEM_TYPE            VARCHAR2(50),
    LAST_PRCR_WI_APPR_DT           DATE,
    PRIOR_STATUS                   VARCHAR2(50),
    PRCR_APPX_LIST                 VARCHAR2(255),
    CREDIT_CARD_TYPE               VARCHAR2(50),
    IS_NTB_CLIENT                  VARCHAR2(5),
    PROD_PROC_SOURCE_IND           VARCHAR2(50),
    G360_ID1                       VARCHAR2(50),
    G360_ID2                       VARCHAR2(50),
    SAR                            VARCHAR2(5),
    DATAVISION                     VARCHAR2(50),
    IS_TDD_PAST_DUE                VARCHAR2(5),
    IS_SUPPLEMENTAL_CARDHOLDERS    VARCHAR2(5),
    IS_ONBOARDED                   VARCHAR2(5),
    IS_RISK_CHNG                   VARCHAR2(5),
    LAST_COMPLETED_PRCR_DATE       DATE,
    IS_CLIENT_SPF                  VARCHAR2(5),
    SHARE_TYPE                     VARCHAR2(50),
    LIST_OF_MAJOR_EXCHANGES        VARCHAR2(255),
    PRIMARY_OPS_CNTRY_CODE         VARCHAR2(10),
    IS_WORDLINK_USAGE_ANTICIPATED  VARCHAR2(5),
    CUSTOMER_RELATIONSHIP_TYPE     VARCHAR2(50),
    BUSINESS_INTERNAL_NAME         VARCHAR2(255),
    PRNT_KYC_VERSION_NUM           NUMBER,
    EXPIRY_DATE                    DATE,
    FOREIGN_CORRESP_BANK           VARCHAR2(5),
    IS_CITI_CORRESPONDENT          VARCHAR2(5),
    IS_CLIENT_CENTRAL_BANK         VARCHAR2(5),
    IS_BEARER_SHARES_ENTITY        VARCHAR2(5),
    BEARER_SHARES_DLNQ_INDICATOR   VARCHAR2(5),
    IS_EXCEPTION_REQUEST           VARCHAR2(5),
    PREV_ETL_STATUS                VARCHAR2(50),
    LEGACY_KYCID                   VARCHAR2(50),
    CLIENT_BANK_TYPE               VARCHAR2(50),
    FINCEN_EXMP_BY_CITIKYC         VARCHAR2(50),
    FINCEN_EXMP_BY_CITIKYC_RESN    VARCHAR2(255),
    CUST_BU                        VARCHAR2(50),
    NAIC_CODES                     VARCHAR2(50),
    LAST_APPROVED_STATUS           VARCHAR2(50),
    DEACTIVATION_DATE              DATE,
    REACTIVATION_DATE              DATE,
    NEW_CITI_SERVICE_CNTRY_CODE    VARCHAR2(10),
    TDD_DUE_DATE                   DATE,
    CIP_DUE_DATE                   DATE,
    IS_CLIENT_APPROVED             VARCHAR2(5),
    FIRST_APPROVAL_DATE            DATE,
    SDD4                           VARCHAR2(50),
    SDD6                           VARCHAR2(50),
    IS_VIP_CUSTOMER                VARCHAR2(5),
    CUSTOMER_LANG_PREFERENCE       VARCHAR2(50),
    SIMPLIFIED_CUST_FLAG           VARCHAR2(5),
    SIMPLIFIED_CUST_LST_UPD_DT     DATE,
    SIMPLIFIED_CRITERIA_FAIL_DT    DATE,
    BASE_RISK_RATING               VARCHAR2(20),
    ANNUAL_REVENUE                 NUMBER,
    ANNUAL_REVENUE_CCY             VARCHAR2(10),
    DEACTIVATION_REASON            VARCHAR2(255),
    CUSTOMER_TRANSFER_FLAG         VARCHAR2(5),
    IS_WHJV_ESTATE                 VARCHAR2(5),
    EXPIRY_TYPE                    VARCHAR2(50),
    CLIENT_SERVICES                VARCHAR2(100),
    SI549_RULE                     VARCHAR2(50),
    SCHEDULED_UNSCHEDULED_PRCR     VARCHAR2(50),
    CUSTOMER_TIER                  VARCHAR2(50),
    IS_EWR_CLIENT                  VARCHAR2(5),
    EXTENSION_EXPIRY_DATE          DATE,
    CHECKER_RVW_STATUS             VARCHAR2(50),
    IS_SANCTIONS_SUSPEND           VARCHAR2(5),
    IS_PROSPECT                    VARCHAR2(5),
    IS_ACCOM_ACCT                  VARCHAR2(5),
    IS_NDA_FLAG                    VARCHAR2(5),
    INCOMPLETE_SOFT_ABANDON_FLAG   VARCHAR2(5),
    MULTIPLE_CCID_IND              VARCHAR2(5),
    DIVESTITURE_FLAG               VARCHAR2(5),
    LEGAL_VEHICLE                  VARCHAR2(100),
    IS_COMPLETN_STATUS_RESET_REQD  VARCHAR2(5),
    INTERFACE_ROUTING_FLAG         VARCHAR2(5),
    GLOBAL_PRCR_APPROVAL_DATE      DATE,
    EXCEPTION_END_DATE             DATE,
    LEGAL_STRUCTURE_INST_TYPE      VARCHAR2(100)
);

