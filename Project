CREATE TABLE AMC_DIRECT_PARENT_HNDOFF (
    GFCID                 VARCHAR2(50),
    DIRECT_PARENT_GFCID   VARCHAR2(50),
    PERCENTAGE_OWNED      NUMBER(5,2),
    CREATED_BY            VARCHAR2(50),
    CREATED_DATE          DATE,
    UPDATED_BY            VARCHAR2(50),
    UPDATED_DATE          DATE,
    CREATE_ID             VARCHAR2(50),
    CREATE_DT             DATE,
    LST_UPD_ID            VARCHAR2(50),
    LST_UPD_DT            DATE,
    VER_NUM               VARCHAR2(10),
    ACT_STRT_DT           DATE,
    ACT_END_DT            DATE,
    KYC_SHARD_NUM         VARCHAR2(10),
    UNIQUE_ID             VARCHAR2(100),
    LST_UPD_TIME          DATE
);
