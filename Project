CREATE TABLE KYC_FILE_LOG (
    ID                VARCHAR2(100),
    FILE_NAME         VARCHAR2(255),
    INBOUND_DIRECTORY VARCHAR2(500),
    ARRIVAL_DATE      DATE,
    TRAILER_COUNT     NUMBER,
    FILE_COUNT        NUMBER,
    COUNT_MATCH       VARCHAR2(10),
    SOURCE_SYSTEM     VARCHAR2(100),
    STATUS            VARCHAR2(50),
    RETRY_COUNT       NUMBER,
    SUMMARY           VARCHAR2(1000),
    EXCEPTION         VARCHAR2(1000),
    CREATE_ID         VARCHAR2(50),
    CREATE_DT         DATE,
    LST_UPD_ID        VARCHAR2(50),
    LST_UPD_DT        DATE,
    VER_NUM           NUMBER,
    ACT_STRT_DT       DATE,
    ACT_END_DT        DATE,
    KYC_SHARD_NUM     NUMBER,
    LST_UPD_TIME      VARCHAR2(20),
    AS_OF_DATE        DATE
);
